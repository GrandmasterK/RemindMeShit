{
  "manifest_version": 2,

  "name": "RemindShit",
  "description": "This extension reminds you to do shit.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "tinylogo.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [ {
  "matches": ["http://*/*"],
  "js": ["Parse.js"]
} ],
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}